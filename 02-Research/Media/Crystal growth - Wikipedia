.hlist dl, .hlist ol, .hlist ul { margin: 0px; padding: 0px; }

.hlist dl dl, .hlist ol dl, .hlist ul dl, .hlist dl ol, .hlist ol ol, .hlist ul ol, .hlist dl ul, .hlist ol ul, .hlist ul ul { display: inline; }

