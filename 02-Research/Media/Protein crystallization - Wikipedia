ol.references { counter-reset: mw-ref-extends-parent 0 mw-references 0 list-item 0; }

ol.references > li { counter-increment: mw-ref-extends-parent 1 mw-references 1; counter-reset: mw-ref-extends-child 0; }

ol.references .mw-subreference-list { list-style: none; margin-left: 1em; padding: 0px; }

.mw-subreference-list > li::before { counter-increment: mw-ref-extends-child 1; content: counter(mw-ref-extends-parent) "." counter(mw-ref-extends-child) ". "; }

sup.reference { unicode-bidi: isolate; white-space: nowrap; font-weight: normal; font-style: normal; }

